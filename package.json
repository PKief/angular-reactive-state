    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "changelog-machine": "^1.0.2",
    "cpx": "^1.5.0",
    "eslint": "^8.28.0",
    "jest": "29.7.0",
    "jest-preset-angular": "^14.0.2",
    "ng-packagr": "^15.0.0",
    "prettier": "latest",
    "tslint-config-prettier": "latest",
    "typescript": "~4.8.2"
  }
